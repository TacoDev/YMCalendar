{
    "name": "ymcalender",
    "version": "0.0.1",
    "description": "Calender of upcoming activities for Sammamish River ward",
    "type": "module",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "rollup -c",
        "build:prod": "npm run clean:build && rollup -c --environment BUILD:production",
        "watch": "rollup -c --watch",
        "serve": "wds --app-index dist --node-resolve --open --watch --port 4200",
        "start": "npm-run-all --parallel watch serve",
        "clean:dev": "npm run clean:build && rimraf node_modules /s /q && npm cache clean --force ",
        "clean:build": "rimraf dist"
    },
    "author": "Ben Carlson",
    "license": "Apache-2.0",
    "dependencies": {
        "@fluentui/react-calendar-compat": "^0.1.2",
        "@fluentui/react-components": "^9.44.5",
        "@fluentui/react-icons": "^2.0.224"
    },
    "peerDependencies": {
        "@fluentui/react": "^8.113.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/react": "^18.0.15",
        "@web/dev-server": "^0.4.3",
        "npm-run-all": "^4.1.5",
        "rimraf": "^5.0.5",
        "rollup": "^4.13.0",
        "rollup-plugin-copy": "^3.5.0",
        "rollup-plugin-postcss": "^4.0.1",
        "typescript": "^4.7.4"
    }
}
